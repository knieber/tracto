{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-tabs": 0,
    "react/prop-types": 0,
    "react/jsx-indent": [
      "error",
      2
    ],
    "react/jsx-indent-props": [
      2,
      "tab"
    ],
    "react/jsx-filename-extension": 0
  }
}